type Query {
  accountList: [BankAccount]
  accountById(id: String): BankAccount
}

type Mutation {
  addBankAccount(input: BankAccountInput): BankAccount
}

input BankAccountInput {
  balance: Float
  currency: String
  type: String
}

type BankAccount {
  id: ID
  createdAt: String
  updatedAt: String
  balance: Float
  currency: String
  type: String
}
